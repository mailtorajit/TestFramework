stage('SonarQube analysis') {
    // requires SonarQube Scanner 3.0.3.778+
    def scannerHome = tool 'SonarQube Scanner 3.0.3.778';
    withSonarQubeEnv('My SonarQube Server') {
      sh "${scannerHome}/bin/sonar-scanner"
    }
  }
   stage('deploy') {
    // some block
    echo 'inside stage'
}

}
